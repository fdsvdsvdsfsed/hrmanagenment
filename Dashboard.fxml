<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="425.0" prefWidth="445.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hrvivek.DashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Dashbord" />
        <Button onAction="#admin" fx:id="adminbutton" mnemonicParsing="false" text="Admin" />
        <Button onAction="#employee" fx:id="employeebutton" mnemonicParsing="false" text="Employee" />
        <Button onAction="#logout" fx:id="logoutbutton" mnemonicParsing="false" text="Logout" />
        <Button onAction="#exit" mnemonicParsing="false" text="Exit" />
    </children>


</VBox>